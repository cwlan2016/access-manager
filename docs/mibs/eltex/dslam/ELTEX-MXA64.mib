mxaDslProfileDsMinNoiseMargin  Integer32,
mxaDslProfileUsMaxNoiseMargin  Integer32,
mxaDslProfileUsNoiseMargin     Integer32,
mxaDslProfileUsMinNoiseMargin  Integer32,
mxaDslProfileStandard          MXADslLineStandard,

mxaDslProfileDsMinInp          Adsl2SymbolProtection,
mxaDslProfileUsMinInp          Adsl2SymbolProtection,
mxaDslProfileDsBitSwap         MXAEnableDisableValue,
mxaDslProfileUsBitSwap         MXAEnableDisableValue,
mxaDslProfileDsRAMode          Adsl2RaMode,
mxaDslProfileUsRAMode          Adsl2RaMode
