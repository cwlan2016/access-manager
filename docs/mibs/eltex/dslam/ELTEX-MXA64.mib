	STATUS current
	DESCRIPTION
		"Represents enable/disabled values combination"
	SYNTAX INTEGER { enabled(1), disabled(2) }

MXADslProfileEntry ::= SEQUENCE {
mxaDslProfileNumber	       Integer32,
mxaDslProfileName              DisplayString,
mxaDslProfileDsMinBitRate      Integer32,
