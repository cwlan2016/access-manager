ELTEX-MXA64-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Gauge32, Counter32, Unsigned32, TimeTicks
        FROM SNMPv2-SMI	
    RowStatus, DisplayString, TruthValue, MacAddress, TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    OBJECT-GROUP
    	FROM SNMPv2-CONF
    Adsl2RaMode, Adsl2SymbolProtection, Adsl2Direction
    	FROM ADSL2-LINE-TC-MIB
		
    elHardware
        FROM ELTEX-SMI;

 mxa64 MODULE-IDENTITY
    LAST-UPDATED "201006150000Z"
    ORGANIZATION "Eltex Co"
    CONTACT-INFO "eltex@gcom.ru"
    DESCRIPTION  "MIB for MXA64 DSLAM"
    REVISION     "201006150000Z"
    DESCRIPTION
		"Initial revision."
    ::= { elHardware 33 }


-- MAIN properties

mxaDevName OBJECT-TYPE
    SYNTAX		DisplayString (SIZE (0..255))
    MAX-ACCESS	read-only
    STATUS		current
    DESCRIPTION
	"it is environment var sysmane"
    ::= { mxa64 1 }

mxaDevType OBJECT-TYPE
    SYNTAX		Integer32
    MAX-ACCESS	read-only
    STATUS		current
    DESCRIPTION
	"device type, for mxa type = 28"
    ::= { mxa64 2 }

mxaDevCfgBuild OBJECT-TYPE
    SYNTAX		DisplayString (SIZE (0..255))
    MAX-ACCESS	read-only
    STATUS		deprecated
    DESCRIPTION
	"OS build string: compiling data and other info"
    ::= { mxa64 3 }

-- BEGIN Sensors (voltage,temperature)
mxaSensor OBJECT IDENTIFIER ::= { mxa64 4 }

-- mxaSensorNumber OBJECT-TYPE
--     SYNTAX  Integer32
--     MAX-ACCESS  read-only
--     STATUS  current
--     DESCRIPTION
-- 	"The number of voltage and temperature sensor"
--     ::= { mxaSensor 1 }

mxaSensorTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF MxaSensorEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
	"A list of sensor entries.  The number of
	entries is given by the value of mxaSensorNumber."
    ::= { mxaSensor 2 }    

mxaSensorEntry OBJECT-TYPE
    SYNTAX  MxaSensorEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
	"An sensor entry containing objects at the sensor description, 
	value range, alarm status and below for a particular sensor."
    INDEX   { mxaSensorIndex }
    ::= { mxaSensorTable 1 }

MxaSensorEntry ::= SEQUENCE {
mxaSensorIndex    Integer32,
mxaSensorDescr    DisplayString,
mxaSensorValue    Integer32,
mxaSensorRangeMin Integer32,
mxaSensorRangeMax Integer32
}

-- BEGIN Sensors members

mxaSensorIndex OBJECT-TYPE
    SYNTAX  Integer32 (1..10)
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"A unique value for each sensor.  Its value
	ranges between 1 and the value of mxaSensorNumber."
    ::= { mxaSensorEntry 1 }

mxaSensorDescr OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (0..100))
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"A textual string containing information about the
	sensor."
    ::= { mxaSensorEntry 2 }

mxaSensorValue OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"Readings of the sensor."
    ::= { mxaSensorEntry 3 }

mxaSensorRangeMin OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"Minimum allowed value of the sensor's readings. If sensor's readings
	are less than this this value, the corresponding trap is sent."
    ::= { mxaSensorEntry 4 }

mxaSensorRangeMax OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"Maximum allowed value of the sensor's readings. If sensor's readings
	are greater than this this value, the corresponding trap is sent."
    ::= { mxaSensorEntry 5 }

-- END Sensors (voltage,temperature)


mxaUptime OBJECT-TYPE
    SYNTAX		DisplayString (SIZE (0..255))
    MAX-ACCESS	read-only
    STATUS		deprecated
    DESCRIPTION
	"Uptime in format dd hh mm ss"
    ::= { mxa64 6 }

