-- BEGIN DSL Port Table


MXADslLineStandard ::= TEXTUAL-CONVENTION
	STATUS current
	DESCRIPTION
		"ADSL 2 standard"
	SYNTAX INTEGER {
		noStandard(0),
		ansit1413(1),
		etsi(2),
		g9921(4),
		g9922(16),
		g9923(64),
		g9923AnnexL(256),
		g9923AnnexM(4096),
		g9925(16384),
		g9925AnnexM(65536),
		multiMode(262143)
	}


mxaDsl OBJECT IDENTIFIER ::= { mxa64 10 }
    
-- mxaDslNumber OBJECT-TYPE
--     SYNTAX  Integer32
--     MAX-ACCESS  read-only
--     STATUS  current
--     DESCRIPTION
-- 	"Number of mxaDslEntry entries in mxaDslTable"
--     ::= { mxaDsl 1 }

mxaDslPortTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF MXADslPortEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
	"A list of port entries.  The number of
	entries is given by the value of mxaDslNumber."
    ::= { mxaDsl 2 }    

mxaDslPortEntry OBJECT-TYPE
    SYNTAX  MXADslPortEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
	"An entry containing all information about particular physical DSL port,
	such as: description, oper status, statistics"
    INDEX   { mxaDslPortNumber }
    ::= { mxaDslPortTable 1 }

MXADslPortEntry ::= SEQUENCE {
mxaDslPortNumber        Integer32,
mxaDslPortName          DisplayString,
mxaDslPortOperStatus    INTEGER,
mxaDslPortAdminStatus   INTEGER,
mxaDslPortStandard      MXADslLineStandard,
mxaDslPortActiveProfile INTEGER,
mxaDslPortIgnLinkAlarm  TruthValue,

-- MXA-24 specific columns
mxaDslPortStatus              INTEGER,
mxaDslPortLatency             INTEGER,
mxaDslPortLclCrcErrors        Counter32,
mxaDslPortRmtCrcErrors        Counter32,
mxaDslPortRmtFecErrors        Counter32,
mxaDslPortCorrectedErrors   Counter32,
mxaDslPortUncorrectedErrors Counter32,
mxaDslPortUtpParityErrors     Counter32
}

mxaDslPortNumber OBJECT-TYPE
    SYNTAX  Integer32 (1..64)
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"Number of the current port."
    ::= { mxaDslPortEntry 1 }

mxaDslPortName OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (0..10))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"The name of the port."
    ::= { mxaDslPortEntry 2 }

mxaDslPortOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    	      up(1),
	      down(2)
	    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The current operational state of the correspondig 
	DSL port."
    ::= { mxaDslPortEntry 3 }

mxaDslPortAdminStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    	      up(1),
	      down(2)
	    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"The desired sate of the corresponding DSL port."
    ::= { mxaDslPortEntry 4 }

mxaDslPortStandard OBJECT-TYPE
    SYNTAX  MXADslLineStandard
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"ADSL standard used on port."
    ::= { mxaDslPortEntry 5 }

mxaDslPortActiveProfile OBJECT-TYPE
    SYNTAX INTEGER {
    	noProfile(0),
	firstProfile(1),
	secondProfile(2),
	thirdProfile(3),
	fourthProfile(4),
	fifthProfile(5),
	sixthProfile(6),
	seventhProfile(7),
	eighthProfile(8),
	ninthProfile(9),
	tenthProdile(10)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	"ADSL Profile currently applied to the port."
    ::= { mxaDslPortEntry 6 }

mxaDslPortIgnLinkAlarm OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	"Ignore link alarm settings. Set this varialble to true(1) 
	in order to prevent agent from sending traps upon this 
	port's status changes."
    ::= { mxaDslPortEntry 7 }

mxaDslPortStatus OBJECT-TYPE
    SYNTAX INTEGER {
    			idle(0),
			activating(1),
			ghs(2),
			training(3),
			showtime(4),
			localLoopback(5),
			analogLoopback(6),
			spectrumMode(7),
			ldmCompleted(8),
			ldmInPorgress(9),
			pmL2(10)
    		}
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"ADSL port status."
    ::= { mxaDslPortEntry 8 }

mxaDslPortLatency OBJECT-TYPE
    SYNTAX INTEGER {
    		fast(1),
		interleaved(2),
		adsl2fast(3),
		adsl2interleaved(4)
           }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"ADSL Port latency."
    ::= { mxaDslPortEntry 9 }

mxaDslPortLclCrcErrors OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Number of local CRC errors."
    ::= { mxaDslPortEntry 10 }

mxaDslPortRmtCrcErrors OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Number of remote CRC errors."
    ::= { mxaDslPortEntry 11 }

mxaDslPortRmtFecErrors OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Number of remote FEC errors."
    ::= { mxaDslPortEntry 12 }

mxaDslPortCorrectedErrors OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Number of corrected errors."
    ::= { mxaDslPortEntry 13 }

mxaDslPortUncorrectedErrors OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Number of uncorrected errors."
    ::= { mxaDslPortEntry 14 }

mxaDslPortUtpParityErrors OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Number of UTP parity errors."
    ::= { mxaDslPortEntry 15 }



mxaDslBandTable OBJECT-TYPE
    SYNTAX SEQUENCE OF MXADslBandEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Table containing upstream/downstream characteristics of the 
	DSL ports."
    ::= { mxaDsl 3 }

mxaDslBandEntry OBJECT-TYPE
    SYNTAX MXADslBandEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"An entry of mxaDslBandTable."
    INDEX { mxaDslBandDirection, mxaDslBandPort }
    ::= { mxaDslBandTable 1 }

MXADslBandEntry ::= SEQUENCE {
mxaDslBandDirection       Adsl2Direction,
mxaDslBandPort            Integer32,
mxaDslBandActualRate      Gauge32,
mxaDslBandAttainableRate  Gauge32,
mxaDslBandSnrMargin       Integer32,
mxaDslBandLineAttenuation Integer32,
mxaDslBandTxPower         Integer32,

mxaDslBandInterleaveDelay   Integer32,
mxaDslBandInp               Adsl2SymbolProtection,
mxaDslBandSignalAttenuation Integer32,
mxaDslBandPayload           Gauge32
}


-- BEGIN ports members

mxaDslBandDirection OBJECT-TYPE
    SYNTAX Adsl2Direction
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Direction of the band."
    ::= { mxaDslBandEntry 1 }

mxaDslBandPort OBJECT-TYPE
    SYNTAX Integer32 (1..32)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Number of the current port."
    ::= { mxaDslBandEntry 2 }

mxaDslBandActualRate OBJECT-TYPE
    SYNTAX Gauge32
    UNITS  "bps"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Actual data rate in the band's direction on current port."
    ::= { mxaDslBandEntry 3 }

mxaDslBandAttainableRate OBJECT-TYPE
    SYNTAX Gauge32
    UNITS  "bps"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Indicates the maximum currently attainable by the port data rate
	in the band's direction. This value will be equal or greater 
	than the downstream line rate."

    ::= { mxaDslBandEntry 4 }

mxaDslBandSnrMargin OBJECT-TYPE
    SYNTAX Integer32
    UNITS "dB"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Signal to noise margin."
    ::= { mxaDslBandEntry 5}

mxaDslBandLineAttenuation OBJECT-TYPE
    SYNTAX Integer32
    UNITS "dB"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Line attenuation."
    ::= { mxaDslBandEntry 6}

mxaDslBandTxPower OBJECT-TYPE
    SYNTAX Integer32
    UNITS "dB"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Transmission power."
    ::= { mxaDslBandEntry 7 }


mxaDslBandInterleaveDelay OBJECT-TYPE
    SYNTAX Integer32
    UNITS  "s"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Interleave delay."
    ::= { mxaDslBandEntry 8 }

mxaDslBandInp OBJECT-TYPE
    SYNTAX Adsl2SymbolProtection
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Impulse noise protection settings."
    ::= { mxaDslBandEntry 9 }

mxaDslBandSignalAttenuation OBJECT-TYPE
    SYNTAX Integer32
    UNITS "dB"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Signal attenuation."
    ::= { mxaDslBandEntry 10 }

mxaDslBandPayload OBJECT-TYPE
    SYNTAX Gauge32
    UNITS "bytes"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Payload size."
    ::= { mxaDslBandEntry 11 }

-- END DSL Port Table	


